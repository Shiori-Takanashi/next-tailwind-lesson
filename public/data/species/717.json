      "version": {
        "name": "x",
        "url": "https://pokeapi.co/api/v2/version/23/"
      }
    },
    {
      "flavor_text": "When this legendary Pokémon’s wings and\ntail feathers spread wide and glow red,\nit absorbs the life force of living creatures.",
      "language": {
        "name": "en",
        "url": "https://pokeapi.co/api/v2/language/9/"
