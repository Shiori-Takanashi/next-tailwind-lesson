      "version": {
        "name": "sword",
        "url": "https://pokeapi.co/api/v2/version/33/"
      }
    },
    {
      "flavor_text": "Es ist zwar noch ein Baby, aber es weiß sich mit\nden Psycho-Kräften, die seinen schleifenförmigen\nFühlern innewohnen, durchaus zu wehren.",
      "language": {
        "name": "de",
        "url": "https://pokeapi.co/api/v2/language/6/"
